@model IEnumerable<onlygodknows.Models.ProjectList>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProjectCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROJECT_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.START_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.END_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.excute_by)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.project_period)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.equipment_budget)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.man_power_budget)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROJECT_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.START_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.END_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.excute_by)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.project_period)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.equipment_budget)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.man_power_budget)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
