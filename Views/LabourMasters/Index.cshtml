@model IEnumerable<onlygodknows.Models.LabourMaster>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
@{
    var i = 0;
}
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EMPNO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ManPowerSupply)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VisaSponser)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Person_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Position)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nationality)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EMPNO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ManPowerSupply)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VisaSponser)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Person_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Position)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nationality)
        </td>
    </tr>
    if (i==100)
    {
        break;
    }
    else
    {
        i++;
    }
}

</table>
